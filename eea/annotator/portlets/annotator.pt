<html xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  tal:omit-tag=""
  i18n:domain="eea">

<dl class="portlet">

  <dt class="portletHeader" tal:condition="view/data/label|nothing">
    <a tal:content="view/data/label"
      tal:attributes="href string:${view/context/absolute_url}"/>
  </dt>

  <dd class="portletItem eea-annotator-portlet">
    <div class="annotator-errata"></div>
  </dd>

</dl>
</html>
